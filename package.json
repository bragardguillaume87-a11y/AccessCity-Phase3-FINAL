{
  "name": "accesscity-phase5",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest --coverage",
    "lint": "eslint src --ext .js,.jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --check \"src/**/*.{js,jsx,json,css}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "predev:vite": "node tools/generate-assets-manifest.js",
    "dev:vite": "vite --config vite.config.js",
    "dev:server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:server\" --names \"VITE,SERVER\" --prefix-colors \"cyan,magenta\"",
    "prebuild:vite": "node tools/generate-assets-manifest.js",
    "build:vite": "vite build --config vite.config.js",
    "build:vite:coverage": "set VITE_COVERAGE=true && vite build --config vite.config.js",
    "preview:vite": "vite preview --config vite.config.js",
    "e2e:vite": "npm run build:vite:coverage && vite preview --port 8000 & playwright test --config=playwright.config.vite.ts",
    "e2e:install": "playwright install --with-deps",
    "coverage": "c8 --reporter=text --reporter=lcov npm test",
    "coverage:html": "c8 --reporter=html npm test",
    "coverage:merge": "node tools/merge_coverage.cjs",
    "coverage:reports": "node tools/generate_reports.cjs",
    "summary": "node tools/generate_summary.js --help",
    "install:server": "cd server && npm install"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "c8": "^9.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "istanbul-lib-coverage": "^3.2.0",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.1.5",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-istanbul": "^7.2.1",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@xyflow/react": "^12.10.0",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dagre": "^0.8.5",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.26",
    "immer": "^11.1.0",
    "lucide-react": "^0.561.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.2",
    "react-hook-form": "^7.69.0",
    "react-resizable-panels": "^4.0.13",
    "react-rnd": "^10.5.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "use-sync-external-store": "^1.6.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1",
    "zundo": "^2.3.0",
    "zustand": "^5.0.9"
  },
  "overrides": {
    "@xyflow/react": {
      "zustand": "^5.0.9"
    }
  }
}
